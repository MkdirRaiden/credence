{
  "$schema": "https://json.schemastore.org/nest-cli.json",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [],
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": { "classValidatorShim": false, "introspectComments": true }
      }
    ]
  }
}
